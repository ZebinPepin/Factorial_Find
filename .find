#include <iostream>

using std::cout;
using std::cin;

int main()
{

        cout << "Enter a number: ";
        int num;
        cin >> num;

        // find factorial
        int ans = 1;
        int i = 1; // variable to count from 1 to num

        while (i <= num) {
                ans = ans * i;
                i = i + 1; }

        cout << "Factorial of  " << num << " is " << ans << "\n";

        return 0;
}
